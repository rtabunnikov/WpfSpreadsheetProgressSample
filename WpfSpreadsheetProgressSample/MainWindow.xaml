<dx:ThemedWindow
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfSpreadsheetProgressSample"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:dxsps="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet" x:Class="WpfSpreadsheetProgressSample.MainWindow"
        mc:Ignorable="d"
        dx:ThemeManager.ThemeName="Office2019Colorful"
        Title="Spreadsheet Progress Sample"
        Height="450"
        Width="800"
        WindowState="Maximized">
    <Grid>
        <dxsps:SpreadsheetControl x:Name="spreadsheetControl1" CommandBarStyle="Ribbon" ShowFormulaBar="True" ShowStatusBar="True" UnhandledException="spreadsheetControl1_UnhandledException"/>
    </Grid>
</dx:ThemedWindow>
